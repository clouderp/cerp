	]
    }],
    "release": [{
	"repo": "git@github.com:clouderp/odoo-cerp_basic",
	"modules": [
	    "addons/cerp_basic",
	    "addons/cerp_core",
	    "addons/cerp_*_basic"
	]
    }]
}
