{
    "addons": [{
	"preserve": [
	    "__manifest__.py",
	    "adapter",
	    "adapter.py",
	    "constants.py",
	    "utils.py",
	    "static",
	    "tests"
	],
	"template": "templates/addon",
	"modules": [
	    "addons/cerp_*_basic"
	]
    }],
